function ra_start

choice = ra_pick;


end

function nextgui = ra_pick

% Construct a questdlg with three options
choice = questdlg('What would you like to do?', ...
	'Reliability Analyses', ...
	'Process new data','View results');
% Handle response
switch choice
    case 'Process new data'
        dessert = 1;
    case 'View results'
        disp([choice ' coming right up.'])
        dessert = 2;
    case 'No thank you'
        disp('I''ll bring you your check.')
        dessert = 0;
end

end